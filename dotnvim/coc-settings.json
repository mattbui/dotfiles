{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "javascripts",
    "json",
    "python"
  ],
  "pyright.organizeimports.provider": "isort",
  "python.formatting.provider": "black",
  "python.linting.flake8Enabled": true,
  "explorer.width": 25,
  "explorer.file.showHiddenFiles": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.icon.enableVimDevicons": true,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "-": "open:split",
    "\\": "open:vsplit",
    "T": "delete",
    "D": "deleteForever",
    "d": "cutFile:toggle",
    ".": "toggleHidden",
    "s": "gitStage",
    "u": "gitUnstage"
  },
  "suggest.completionItemKindLabels": {
    "method": "  ",
    "function": "  ",
    "variable": "[]",
    "field": "  ",
    "typeParameter": "<>",
    "constant": "  ",
    "class": " פּ ",
    "interface": " 蘒",
    "struct": "  ",
    "event": "  ",
    "operator": "  ",
    "module": "  ",
    "property": "  ",
    "enum": " 練",
    "reference": "  ",
    "keyword": "  ",
    "file": "  ",
    "folder": " ﱮ ",
    "color": "  ",
    "unit": " 塞 ",
    "snippet": "  ",
    "text": "  ",
    "constructor": "  ",
    "value": "  ",
    "enumMember": "  "
  },
  "suggest.snippetIndicator": "...",
  "snippets.ultisnips.usePythonx": false,
  "git.changedSign.text":"▐",
  "git.addedSign.text":"▐",
  "git.removedSign.text":"▐",
  "git.topRemovedSign.text":"▐",
  "git.changeRemovedSign.text":"▐"
}
